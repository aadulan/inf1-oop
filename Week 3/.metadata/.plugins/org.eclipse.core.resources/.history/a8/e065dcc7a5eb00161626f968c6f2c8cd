import java.util.Arrays;

/**
 * 
 */

/**
 * @author s1631442
 *
 */
public class TempMedian {
	
	public static void main(String[] args){
		int N = args.length;
		int[] nums = new int[N];
		
		for (int i = 0; i<N; i++){
			
			nums[i] = Integer.parseInt(args[i]);
		}
		 
		for (int i =0; i<N; i++){
		
		System.out.print( nums[i] + " ");
		 }
		Arrays.sort(nums);
		
		System.out.println(" ");
		
		for(int i= 0 ;i<N ; i++){
			System.out.print(nums[i] + " ");
		}
	}

}
